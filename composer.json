{
    "name": "arokettu/arithmetic-parser",
    "type": "library",
    "description": "Configurable arithmetic parser and interpreter",
    "keywords": ["arithmetic", "maths", "math", "parser", "interpreter"],
    "license": "MIT",
    "homepage": "https://sandfox.dev/php/arithmetic-parser.html",
    "authors": [{
        "name": "Anton Smirnov",
        "email": "sandfox@sandfox.me",
        "homepage": "https://sandfox.me/",
        "role": "developer"
    }],
    "support": {
        "issues": "https://gitlab.com/sandfox/arithmetic-parser/-/issues",
        "source": "https://gitlab.com/sandfox/arithmetic-parser",
        "docs": "https://php-arithmetic-parser.readthedocs.io/",
        "chat": "https://gitter.im/arokettu/community"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\ArithmeticParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\ArithmeticParser\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/lexer": "^2 | ^3",
        "php-ds/php-ds": "^1.4"
    },
    "suggest": {
        "ext-ds": "More efficient data structures"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "psy/psysh": "*",
        "sandfox.dev/code-standard": "^1",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.6"
    }
}
