{
    "name": "arokettu/arithmetic-parser",
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\ArithmeticParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\ArithmeticParser\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/lexer": "^2 | ^3",
        "php-ds/php-ds": "^1.4"
    },
    "suggest": {
        "ext-ds": "More efficient data structures"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
